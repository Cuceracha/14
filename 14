var f:file of char;
    c:char;
    i,k:integer;
begin
assign(f,'f:\lr14\mfile.txt');
rewrite(f);
write('Ğ¡ĞºĞ¾Ğ»ÑŒĞºĞ¾ ÑĞ¸Ğ¼Ğ²Ğ¾Ğ»Ğ¾Ğ² Ğ·Ğ°Ğ¿Ğ¸ÑĞ°Ñ‚ÑŒ Ğ² Ñ„Ğ°Ğ¹Ğ» k=');
read(k);
writeln('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ Ğ² Ñ„Ğ°Ğ¹Ğ» ',k,' ÑĞ¸Ğ¼Ğ²Ğ¾Ğ»Ğ¾Ğ², Ğ² Ñ‚Ğ¾Ğ¼ Ñ‡Ğ¸ÑĞ»Ğµ Ğ¿Ñ€Ğ¾Ğ±ĞµĞ»:');
for i:=1 to k do
 begin
  read(c);
  write(f,c);
 end;
reset(f);
k:=-1;
for i:=0 to filesize(f)-1 do
 begin
  read(f,c);
  if c=' ' then k:=i;
 end;
if k=-1 then
 begin
  writeln('ĞĞµ Ğ²Ğ²ĞµĞ´ĞµĞ½ Ğ¿Ñ€Ğ¾Ğ±ĞµĞ»');
  close(f);
 end
else
 begin
  seek(f,k);
  truncate(f);
  seek(f,0);
  close(f);
 end;
end.
var
 S: ShortString;
 var input,output: file of ShortString;
 MaxLen, i: integer;
begin
  Assign(input,'input.txt');
  Rewrite(input);
   write('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ÑÑ‚Ñ€Ğ¾ĞºĞ¸ Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°:');
  for i:=1 to 5 do
  begin
       readln(s);
       write(input,s);
  end;
  Reset(input);
  Assign(output,'output.txt');
  Rewrite(output);
 
 MaxLen:=-1;
 
 
 while (not eof(input)) do
  begin
   Read(input,S);
   if (Length(S)>MaxLen) or (MaxLen=-1) then
   MaxLen:=Length(S);
  end;
 
 Close(input);
 Reset(input);
 
 
 while (not eof(input)) do
  begin
   Read(input,S);
   if Length(S)=MaxLen then
   Write(output,S);
  end;
 
 Close(input);
 Close(output);
end.
var f,g,h:file of real;
    n, i:integer;
    a:real;
begin
randomize;
assign(f,'file1');
rewrite(f);
write('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾ Ñ‡Ğ¸ÑĞµĞ» Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°: ');
readln(n);
for i:=1 to n do
 begin
  a:=10*random;
  write(f,a);
 end;
randomize;
reset(f);
assign(g,'file2');
rewrite(g);
assign(h,'file3');
rewrite(h);
writeln('Ğ¡Ğ¾Ğ´ĞµÑ€Ğ¶Ğ°Ğ½Ğ¸Ğµ Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°:');
for i:=0 to filesize(f)-1 do
 begin
  read(f,a);
  write(a:0:2,' ');
  if odd(i)then write(g,a)
  else write(h,a)
 end;
writeln;
writeln;
seek(g,0);
writeln('Ğ¡Ğ¾Ğ´ĞµÑ€Ğ¶Ğ°Ğ½Ğ¸Ğµ Ñ„Ğ°Ğ¹Ğ»Ğ° Ñ‡Ğ¸ÑĞµĞ» Ñ Ñ‡ĞµÑ‚Ğ½Ñ‹Ğ¼Ğ¸ Ğ½Ğ¾Ğ¼ĞµÑ€Ğ°Ğ¼Ğ¸:');
while not eof(g) do
 begin
  read(g,a);
  write(a:0:2,' ');
 end;
close(g);
writeln;
writeln;
seek(h,0);
writeln('Ğ¡Ğ¾Ğ´ĞµÑ€Ğ¶Ğ°Ğ½Ğ¸Ğµ Ñ„Ğ°Ğ¹Ğ»Ğ° Ñ‡Ğ¸ÑĞµĞ» Ñ Ğ½ĞµÑ‡ĞµÑ‚Ğ½Ñ‹Ğ¼Ğ¸ Ğ½Ğ¾Ğ¼ĞµÑ€Ğ°Ğ¼Ğ¸:');
while not eof(h) do
 begin
  read(h,a);
  write(a:0:2,' ');
 end;
close(h);
end.
var f,g:file of real;
    n, i:integer;
    a,s:real;
begin
randomize;
assign(f,'file4');
rewrite(f);
write('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾ Ñ‡Ğ¸ÑĞµĞ» Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°: ');
readln(n);
for i:=1 to n do
 begin
  a:=10*random;
  write(f,a);
 end;
reset(f);
writeln('Ğ¡Ğ¾Ğ´ĞµÑ€Ğ¶Ğ°Ğ½Ğ¸Ğµ Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°:');
s:=0;
for i:=0 to filesize(f)-1 do
 begin
  read(f,a);
  write(a:0:2,' ');
  if i mod 2 = 1 then s:=s+a;
 end;writeln;
 writeln(s:0:2);
end.
var f,g:file of real;
n, i:integer;
a,s, max:real;
var mas: array of real;
begin
randomize;
assign(f,'maximum');
rewrite(f);
write('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾ Ñ‡Ğ¸ÑĞµĞ» Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°: ');
readln(n);
SetLength (mas,n);
for i:=1 to n do
begin
a:=10*random;
write(f,a);
mas[i-1]:=trunc(a * 100 + 0.5)/100; 
end;
max:=mas[0];
for i:=0 to n-1 do 
begin
  if i=0 then
    if mas[1]>max then
      max:=mas[1];
      if (i> 0) and (i<n-1) then
        begin
        if (mas[i]>mas[i-1]) and (mas[i]>mas[i+1]) then
          begin
          max:=mas[i];
          end;
          end;
        if mas[n-1]>mas[n-2] then max:=mas[n-1];
end;
print(mas);
print(max);
end.
var f,g:file of real;
n, i, maxi,mini:integer;
a,s, max, min:real;
var mas: array of real;
begin
randomize;
assign(f,'max');
rewrite(f);
write('Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾ Ñ‡Ğ¸ÑĞµĞ» Ğ¸ÑÑ…Ğ¾Ğ´Ğ½Ğ¾Ğ³Ğ¾ Ñ„Ğ°Ğ¹Ğ»Ğ°: ');
readln(n);
SetLength (mas,n);
for i:=1 to n do
begin
a:=10*random;
write(f,a);
mas[i-1]:=trunc(a * 100 + 0.5)/100; 
end;
println(mas);
min:=mas[0];
mini:=1;
for i:=1 to n-1 do begin
 if mas[i]<min then begin
 min:=mas[i];
 mini:=i+1;
 end;
 end;
 max:=mas[0];
 maxi:=1;
for i:=1 to n-1 do
 if mas[i]>max then begin
 max:=mas[i];
 maxi:=i+1;
 end;
 assign(g,'max1');
rewrite(g);
for i:=1 to n do
begin
  if (i = maxi) then
    write(g,mas[mini-1])
  else 
  begin
  if (i=mini) then
    write(g,mas[maxi-1])
  else 
  write(g,mas[i-1]);
  end;
  end;
  reset(g);
  writeln('Ğ¡Ğ¾Ğ´ĞµÑ€Ğ¶Ğ°Ğ½Ğ¸Ğµ Ñ„Ğ°Ğ¹Ğ»Ğ° Ñ‡Ğ¸ÑĞµĞ» g :');
while not eof(g) do
 begin
  read(g,a);
  write(a:0:2,' ');
 end;
close(g);
close(f);
end.
var
    f: file of char;
    c: char;
    i: integer;
begin
    assign (f, 'text.txt');
    reset (f);
    c:='!';
    for i:=0 to filesize(f)-1 do
      begin
        if i mod 2 = 1 then
          begin
          seek(f,i);
          write(f,c);
        end;
        end;
    close(f);
 end.
